                                <div class="col-md-12">
                                    <input class="form-control" placeholder="Password" type="password" name="password"></input>
                                </div>
                            </div>
                            <br>
                            <div class="row">
                                <div class="col-md-12">
                                    <select class="form-control" name="rol">
